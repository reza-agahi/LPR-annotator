<div class="plate-t1 w3-section w3-row w3-center">
  <div class="plate-t1-background">
    <div class="plate-t1-left-panel">
      <input ng-model="currentPlate.boxes[0].symbol" type="number" name="quantity" min="0" max="9" ng-disabled="currentPlate.boxes.length < 1">
      <input ng-model="currentPlate.boxes[1].symbol" type="number" name="quantity" min="0" max="9" ng-disabled="currentPlate.boxes.length < 2">
      <input ng-model="currentPlate.boxes[2].symbol" type="text" maxlength="3" ng-disabled="currentPlate.boxes.length < 3">
      <input ng-model="currentPlate.boxes[3].symbol" type="number" name="quantity" min="0" max="9" ng-disabled="currentPlate.boxes.length < 4">
      <input ng-model="currentPlate.boxes[4].symbol" type="number" name="quantity" min="0" max="9" ng-disabled="currentPlate.boxes.length < 5">
      <input ng-model="currentPlate.boxes[5].symbol" type="number" name="quantity" min="0" max="9" ng-disabled="currentPlate.boxes.length < 6">
    </div>
    <div class="plate-t1-right-panel">
      <input ng-model="currentPlate.boxes[6].symbol" type="number" name="quantity" min="0" max="9" ng-disabled="currentPlate.boxes.length < 7">
      <input ng-model="currentPlate.boxes[7].symbol" type="number" name="quantity" min="0" max="9" ng-disabled="currentPlate.boxes.length < 8">
    </div>
  </div>
</div>
